for_window [app_id="^lxqt-.*$"] floating enable
exec swaybg -i /usr/share/lxqt/wallpapers/waves-logo.png
exec lxqt-notificationd
exec lxqt-config-session && sway exit
exec notify-send -a 'Welcome to LXQt - Wayland Session' -i dialog-information -t 30000 'No compositor is selected, please choose one in Wayland Settings.' 'The session will be restarted after closing the window.'
